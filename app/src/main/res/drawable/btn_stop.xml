<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="30dp"
        android:height="30dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/outerRing"/>
        </shape>
    </item>
    <item
        android:width="3dp"
        android:height="12dp"
        android:left="10dp"
        android:top="9.5dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/innerRing"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
    <item
        android:width="3dp"
        android:height="12dp"
        android:left="16dp"
        android:top="9.5dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/innerRing"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
</layer-list>